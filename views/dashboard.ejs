                        <div class="x_content">
                            <table id="datatable" class="table table-hover">
                                <thead>
                                    <tr>
                                      <th>Host</th>
                                      <th>Domain Name / IP Address</th>
                                      <th>Status</th>
                                      <th>Check Service Option</th>
                                      <th>Last Check</th>
                                      <th>Duration</th>
                                      <th>Delete Host</th>
                                    </tr>
                                </thead>

                                <tbody id="tableContent">
                                  <% for (host in hosts) { %>
                                    <tr>
                                        <td><i class="fa fa-desktop"></i> &nbsp;&nbsp;<button type="button" class="btn btn-xs btn-link" data-toggle="modal" data-target="#viewModal" data-h_name="<%= hosts[host].displayName %>" data-h_id="<%= hosts[host].id %>"><%= hosts[host].displayName %></button></td>
                                        <td><%= hosts[host].host %></td>
                                        <td>
                                          <% if (hosts[host].status === "Up"){ %>
                                            <i class="fa  fa-check-circle" style="color:rgb(31, 182, 81)"></i> Up
                                          <% } else if (hosts[host].status === "Down") {%>
                                            <i class="fa  fa-times-circle" style="color:rgb(254, 16, 45)"></i> Down
                                          <% } else { %>
                                            <i class="fa  fa-refresh" style="color:rgb(46, 166, 236)"></i> Loading
                                          <% }%>
                                        </td>
                                        <td><%= hosts[host].checkServiceOption %></td>
                                        <td><%= new Date(hosts[host].lastCheckTime).toLocaleString() %></td>
                                        <td><%
                                              var interval = Date.now() - new Date(hosts[host].statusStartTime).getTime();
  
                                              var msecPerMinute = 1000 * 60;  
                                              var msecPerHour = msecPerMinute * 60;  
                                              var msecPerDay = msecPerHour * 24;  

                                              var days = Math.floor(interval / msecPerDay );  
                                              interval = interval - (days * msecPerDay );  

                                              var hours = Math.floor(interval / msecPerHour );  
                                              interval = interval - (hours * msecPerHour );  

                                              var minutes = Math.floor(interval / msecPerMinute );  
                                              interval = interval - (minutes * msecPerMinute );  

                                              var seconds = Math.floor(interval / 1000 );  

                                              var duration = days + "d, " + hours + "h, " + minutes + "m, " + seconds + "s.";  
                                            %>
                                            <%= duration %>
                                        </td>
                                        <td><button type="button" value="<%= hosts[host].id %>" class="deleteHostBtn btn btn-xs btn-danger"><i class="fa fa-trash-o"></i> Delete</button></td>
                                    </tr>
                                  <% } %>
                                </tbody>
                            </table>
                        </div>
