    <!-- addHostModal -->
    <div class="modal fade" id="addHostModal" tabindex="-1" role="dialog" aria-labelledby="addHostModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
            <div class="modal-header">
                <h3 class="modal-title" id="addHostModalLabel" style="display: inline-block;">Add Host</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form class="form-horizontal form-label-left" novalidate>

                    <p>Please fill out following information for adding a new host.</p>
                    <br>

                    <div class="item form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="displayName">Display Host Name <span class="required">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            <input id="displayName" class="form-control col-md-7 col-xs-12" name="hostName" required="required" type="text" placeholder="e.g. My-PC">
                        </div>
                    </div>

                    <div class="item form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="host">Domain Name / IP Address <span class="required">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            <input id="host" class="form-control col-md-7 col-xs-12" name="host" required="required" type="text" placeholder="e.g. www.google.com">
                        </div>
                    </div>

                    <div class="item form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="checkService">Check Service Option<span class="required">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            <select id="checkService" class="form-control col-md-7 col-xs-12" name="checkService" required="required">
                              <option value="Nmap">Nmap</option>
                              <option value="Ping">Ping</option>
                            </select>
                        </div>
                    </div>

                    <div id="contact_box">
                        <!-- Contact select -->
                    </div>

                    <div class="item form-group">
                        <div class="col-md-6 col-sm-6 col-xs-12 col-md-offset-3">
                            <button class="btn btn-success" type="button" onclick="joinContact()">Add Contact</button>
                        </div>
                    </div>
                    
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>
                <button type="button" id="addHostBtn" class="btn btn-success">Add Host</button>
            </div>
            </div>
        </div>
